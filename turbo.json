{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "build:style": {
      "cache": false,
      "outputs": []
    },
    "build:style:dark": {
      "cache": false,
      "outputs": []
    },
    "dev:style": {
      "cache": false,
      "outputs": []
    },
    "dev:style:dark": {
      "cache": false,
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
